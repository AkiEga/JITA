{
  "name": "electron-react-parcel",
  "version": "1.0.0",
  "description": "Electron app build using ReactJS and Parcel web application bundler",
  "main": "src/electron.js",
  "scripts": {
    "webpack-debug-build": "webpack --config webpack.config.js --mode development",
    "react-start": "parcel -p 3000 index.html --out-dir build",
    "watch": "parcel watch index.html",
    "react-build": "parcel build index.html --out-dir build --public-url ./ --no-minify",
    "electron-build": "electron-builder -mwl",
    "clean-build": "rm -rf build/ .cache dist/",
    "build": "yarn clean-build && yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron . \""
  },
  "keywords": [
    "boskysoft",
    "electron",
    "react",
    "parcel",
    "electron-react-parcel",
    "electron react parcel"
  ],
  "author": "Yogesh Kumar <yogesh@boskysoft.com>",
  "license": "MIT",
  "dependencies": {
    "components": "^0.1.0",
    "electron-is-dev": "1.0.1",
    "fs-promise": "^2.0.3",
    "react": "^16.8.0",
    "react-bootstrap-table-next": "^3.1.8",
    "react-data-grid": "^6.0.1",
    "react-data-grid-addons": "^6.1.0",
    "react-dom": "^16.8.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "request-promise": "^4.2.4",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "^0.88.0"
  },
  "build": {
    "appId": "com.boskysoft",
    "files": [
      "src/electron.js",
      "src/assets/*",
      "build/**/*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "src/assets/logo.png"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/logo.png"
    },
    "linux": {
      "target": "deb",
      "icon": "src/assets/logo.png",
      "category": "Development"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@craco/craco": "^5.3.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "concurrently": "4.1.0",
    "craco-less": "^1.12.0",
    "cross-env": "5.2.0",
    "css-loader": "^3.2.0",
    "electron": "9.4.0",
    "electron-builder": "20.38.4",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "parcel-bundler": "1.12.3",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "sass-loader": "^7.2.0",
    "semantic-ui-less": "^2.4.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "wait-on": "3.2.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  }
}
